{
  "name": "cloreai",
  "version": "0.1.1",
  "description": "CLORE.AI API client",
  "keywords": [
    "cloreai",
    "clore",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladpuz/cloreai.git"
  },
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check": "npm run typecheck && npm run lint && npm run format",
    "clean": "rimraf build",
    "format": "prettier --check .",
    "lint": "eslint .",
    "prepack": "npm run clean && npm run build && npm run check",
    "prepare": "husky",
    "start-playground": "node playground/main.js",
    "typecheck": "tsc --noEmit",
    "update-check": "npm-check-updates --peer",
    "update-latest": "npm-check-updates --peer --upgrade --target latest",
    "update-minor": "npm-check-updates --peer --upgrade --target minor"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-vladpuz": "^0.7.0",
    "husky": "^9.0.11",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.3.3",
    "prettier-config-vladpuz": "^0.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  }
}
